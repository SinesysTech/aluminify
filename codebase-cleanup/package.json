{
  "name": "codebase-cleanup-analyzer",
  "version": "1.0.0",
  "description": "Systematic codebase cleanup and refactoring analysis tool for Next.js/Supabase applications",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:property": "vitest run --dir tests/property",
    "test:integration": "vitest run --dir tests/integration",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "codebase-analysis",
    "refactoring",
    "technical-debt",
    "code-quality",
    "static-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ts-morph": "^24.0.0",
    "minimatch": "^10.0.1",
    "commander": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/minimatch": "^5.1.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0",
    "fast-check": "^4.5.3",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0"
  }
}
